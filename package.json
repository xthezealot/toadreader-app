{
  "name": "toadreader",
  "version": "1.0.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "update": "bun update && expo install --fix"
  },
  "dependencies": {
    "@tamagui/babel-plugin": "^1.125.2",
    "@tamagui/config": "^1.125.2",
    "@tamagui/metro-plugin": "^1.125.2",
    "expo": "^52.0.35",
    "expo-constants": "^17.0.6",
    "expo-linking": "^7.0.5",
    "expo-router": "^4.0.17",
    "expo-status-bar": "^2.0.1",
    "install": "^0.13.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.7",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "^0.19.13",
    "tamagui": "^1.125.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@types/react": "~18.3.18",
    "typescript": "^5.7.3"
  },
  "resolutions": {
    "metro": "^0.81.0",
    "metro-resolver": "^0.81.0",
    "metro-config": "^0.81.0"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": false,
    "trailingComma": "all"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false,
        "exclude": [
          "tamagui",
          "@tamagui/babel-plugin",
          "@tamagui/config",
          "@tamagui/metro-plugin"
        ]
      }
    }
  }
}
